{% extends 'AcmeDemoBundle::layout.html.twig' %}

{% block title %}Symfony - Welcome{% endblock %}

{% block content_header 'GoogleMap API Demo' %}


{% block content %}
    {% if google_maps.hasMaps() %}
        {% for map in google_maps.getMaps() %}
            {% if loop.first %}
                {{ map.getGoogleMapLibrary | raw }}
            {% endif %}
            {% autoescape false %}
                {{ map.render }}
            {% endautoescape %}
        {% endfor %}
    {% endif %}
{% endblock %}
